{
  "kind": "collectionType",
  "collectionName": "categories",
  "info": {
    "singularName": "category",
    "pluralName": "categories",
    "displayName": "Category",
    "description": ""
  },
  "options": {
    "draftAndPublish": true,
    "populateCreatorFields": true,
    "timestamps": true
  },
  "pluginOptions": {},
  "attributes": {
    "category_name": {
      "type": "string"
    },
    "category_image": {
      "type": "string"
    },
    "move": {
      "type": "string"
    },
    "sub_categories": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::sub-category.sub-category",
      "mappedBy": "category_id"
    },
    "products": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::product.product",
      "mappedBy": "category_id"
    },
    "file": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": ["images", "files", "videos", "audios"]
    },
    "created_by_user": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "plugin::users-permissions.user"
    },
    "updated_by_user": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "plugin::users-permissions.user"
    }
  }
}
