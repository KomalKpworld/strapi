{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "singularName": "product",
    "pluralName": "products",
    "displayName": "Product",
    "description": ""
  },
  "options": {
    "draftAndPublish": true,
    "populateCreatorFields": true,
    "timestamps": true
  },
  "pluginOptions": {},
  "attributes": {
    "category_id": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::category.category",
      "inversedBy": "products"
    },
    "sub_category_id": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::sub-category.sub-category",
      "inversedBy": "products"
    },
    "image": {
      "type": "string"
    },
    "frame_type": {
      "type": "enumeration",
      "enum": [
        "Free",
        "Paid"
      ]
    },
    "file": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    }
  }
}
